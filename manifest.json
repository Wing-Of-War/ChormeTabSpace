{
	"manifest_version": 2,

	"name":"TagSpace",
	"description": "This extension save batch of tabs as workspace.",
	"version":"1.0",

 	"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  	},

  	"permissions":[
      "tabs",
      "storage"
  	]
}